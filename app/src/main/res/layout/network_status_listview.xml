<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/background_material_dark"
              android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style ="@style/NT_FontNetworkStatus"
        android:text=""
        android:id="@+id/bssid_textView"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style ="@style/NT_FontNetworkStatus"
        android:text=""
        android:id="@+id/ssid_textView"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style ="@style/NT_FontNetworkStatus"
        android:text=""
        android:id="@+id/capabilities_textView"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style ="@style/NT_FontNetworkStatus"
        android:text=""
        android:id="@+id/frequency_textView"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style ="@style/NT_FontNetworkStatus"
        android:text=""
        android:id="@+id/level_textView"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style ="@style/NT_FontNetworkStatus"
        android:text=""
        android:id="@+id/timestamp_textView"/>
</LinearLayout>
